---
import NotesData from "../../components/Data/notes.json"
interface Props {
  notes: typeof NotesData
}
const { notes } = Astro.props as Props
---

{
  notes.map((note) => (
    <article class="p-6 bg-white rounded-3xl border border-gray-200 shadow-card">
      <div class="flex justify-between items-center mb-5 text-gray-500">
        {note.id} id
        <br />
        {note.faculty} faculty
        <br />
        {note.semester} semester
        <br />
        {note.subjects.map((sub) => (
          <div>
            <span>
              {sub.name} <br />
            </span>
            <span>
              <a href={sub.link}>Download</a>
              <br />
            </span>
          </div>
        ))}
      </div>
    </article>
  ))
}
